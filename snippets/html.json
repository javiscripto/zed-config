{
  "Basic HTML template": {
    "prefix": "html",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"en\">",
      "<head>",
      "    <meta charset=\"UTF-8\">",
      "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "    <title>${1:Document}</title>",
      "     <link rel=\"icon\"  href=\"#\"  />",
      "    <link rel=\"stylesheet\" href=\"${2:style.css}\">",
      "</head>",
      "<body>",
      "    ${3:}",
      "</body>",
      "</html>"
    ],
    "description": "Basic HTML template with CSS link"
  },
  "HTML form": {
    "prefix": "form",
    "body": [
      "<form action=\"${1:/url}\" method=\"${2:post}\">",
      "    <input type=\"text\" id=\"\" name=\"\" required>",
      "    <button type=\"submit\">Submit</button>",
      "</form>"
    ],
    "description": "html form with submit"
  },
  "input number": {
    "prefix": "input:number",
    "body": ["<input type=\"number\" id=\"\" name=\"\" required>"],
    "description": "input type number"
  },
  "input text": {
    "prefix": "input:text",
    "body": ["<input type=\"text\" id=\"\" name=\"\" required>"],
    "description": "input type text"
  },
  "input password": {
    "prefix": "input:password",
    "body": ["<input type=\"password\" id=\"\" name=\"\" required>"],
    "description": "input type password"
  },
  "input email": {
    "prefix": "input:email",
    "body": ["<input type=\"email\" id=\"\" name=\"\" required>"],
    "description": "input type email"
  },
  "input date": {
    "prefix": "input:date",
    "body": ["<input type=\"date\" id=\"\" name=\"\" required>"],
    "description": "input type date"
  },
  "button": {
    "prefix": "button",
    "body": ["<button> ${1:} </button>"],
    "description": "create default button"
  },
  "HTML div": {
    "prefix": "div",
    "body": ["<div>", "    ${1:Content}", "</div>"],
    "description": "HTML div tag"
  },
  "class div": {
    "prefix": "class",
    "body": ["<div class=\"${1:class}\">", "    ${2:content}", "</div>"],
    "description": "Create a div with a dynamic class"
  },
  "id div": {
    "prefix": "id",
    "body": ["<div id=\"${1:id}\">", "    ${2:content}", "</div>"],
    "description": "Create a div with a dynamic id"
  },
  "lorem": {
