  },
  "Unordered List": {
    "prefix": "ul",
    "body": [
      "<ul>",
      "    <li>${1:Item 1}</li>",
      "    <li>${2:Item 2}</li>",
      "    <li>${3:Item 3}</li>",
      "</ul>"
    ],
    "description": "Create an unordered list"
  },
  "Ordered List": {
    "prefix": "ol",
    "body": [
      "<ol>",
      "    <li>${1:Item 1}</li>",
      "    <li>${2:Item 2}</li>",
      "    <li>${3:Item 3}</li>",
      "</ol>"
    ],
    "description": "Create an ordered list"
  },
  "Image": {
    "prefix": "img",
    "body": [
      "<img src=\"${1:image.png}\" alt=\"${2:Description}\" width=\"${3:300}\" height=\"${4:200}\">"
    ],
    "description": "Create an image tag"
  },
  "Anchor with text": {
    "prefix": "a:text",
    "body": ["<a href=\"${1:#}\">${2:Link Text}</a>"],
    "description": "Create a text link"
  },
  "Table": {
    "prefix": "table",
    "body": [
      "<table>",
      "    <thead>",
      "        <tr>",
      "            <th>${1:Header 1}</th>",
      "            <th>${2:Header 2}</th>",
      "            <th>${3:Header 3}</th>",
      "        </tr>",
      "    </thead>",
      "    <tbody>",
      "        <tr>",
      "            <td>${4:Data 1}</td>",
      "            <td>${5:Data 2}</td>",
      "            <td>${6:Data 3}</td>",
      "        </tr>",
      "    </tbody>",
      "</table>"
    ],
    "description": "Create a basic HTML table"
  }
}
